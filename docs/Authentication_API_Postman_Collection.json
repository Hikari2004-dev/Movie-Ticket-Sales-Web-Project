  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"phoneNumber\": \"0901234567\",\n  \"password\": \"SecurePass123!\",\n  \"fullName\": \"Nguyen Van A\",\n  \"dateOfBirth\": \"1995-05-15\",\n  \"gender\": \"MALE\",\n  \"privacyPolicyAccepted\": true,\n  \"privacyPolicyVersion\": \"1.0\",\n  \"termsOfServiceAccepted\": true,\n  \"termsOfServiceVersion\": \"1.0\"\n}"
            },
            "url": {
              "raw": "http://localhost:8080/api/auth/register",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8080",
              "path": ["api", "auth", "register"]
            }
          }
        },
